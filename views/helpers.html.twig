{% macro link(link) %}
    <a href="{{ link }}">{{ link }}</a>
{% endmacro %}

{% macro email(email) %}
    <a href="mailto:{{ email }}">{{ email }}</a>
{% endmacro %}

{% macro telephone(tel) %}
    <a href="tel:{{ tel }}">{{ tel }}</a>
{% endmacro %}

{% macro conditionalListItem(field) %}
    {% if field is not empty %}
        <li>{{ field }}</li>
    {% endif %}
{% endmacro %}

{% macro filedownload(field) %}
    {% if field is not empty %}
        <li>{{ field | wufoo_upload }}</li>
    {% endif %}
{% endmacro %}